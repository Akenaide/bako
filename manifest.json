{
  "name": "bako",
  "version": "3.0",
  "description": "Gbf auto refresh on attack",
  "permissions": [
    "webRequest",
    "tabs",
    "declarativeContent",
    "storage",
    "alarms",
    "scripting",
    "bookmarks"
  ],
  "host_permissions": [
    "http://game.granbluefantasy.jp/*"

  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "options_page": "popup.html",
  "commands": {
    "toggle-aracarum-mode": {
      "suggested_key": {
        "default": "Ctrl+1",
        "mac": "Command+1"
      },
      "description": "Toggle arcarum mode"
    },
    "deactivate-all": {
      "suggested_key": {
        "default": "Ctrl+0",
        "mac": "Command+0"
      },
      "description": "Deactivate all options"
    }
  },
  "manifest_version": 3
}
